/*---------预设-----------------------------------*/
body {color:#333;font-size:14px;font-family:"Microsoft YaHei";cursor: default;}
p,h1,h2,h3,h4,h5,h6{margin:0px;}
ul{list-style:none;padding:0px;margin:0px;}
a{text-decoration:none;}
a:link {text-decoration:none;}
a:visited {text-decoration:none;}
a:hover {text-decoration:none;}
ol,ul {list-style-type: none;}
table {border-collapse: collapse;border-spacing: 0;}
input,select,button,textarea{outline: none}
button{border: none;height:32px;color: white;padding: 0 10px;}
textarea{resize:none;}

/*文本框*/
.def-txt2{background-color: transparent;border-right: none;border-left:solid 2px;border-top: solid 2px;border-bottom: solid 2px}
.def-txt{border:solid 2px #7A7A7A;background-color: transparent;}
.def-txt,.def-txt2{height: 32px;padding: 3px 5px;font-size: 14px}
.def-txt:focus,.def-txt2:focus{background-color: white}

/*单选*/
.p-radio{}
.p-radio>span{display:inline-block;padding:0 10px;text-align:center;cursor:pointer;}
.p-radio .sp-lose{background-color:#DDD;color:gray;}
.p-radio .s-bg{color:white;}

/*取消按钮*/
.but-cancel{color:#333;}

/*插入的音频视频标签*/
.f-AV{cursor: pointer;font-size: 1.5em}

/*弹出窗口标题和底部*/
.dlg-tt{position: absolute;top: 0px;left: 0px;height: 30px;line-height: 30px;width: 100%;text-indent: .5em;}
.dlg-ft{text-align: center;border-top-style:solid;border-top-width:1px;padding: .3em .5em;text-align: right;}

/*预设布局*/
.g-l-r{position: relative;}
.g-l-r>div:first-child{position: absolute;top: 0;left: 0px;}
/*-----全局黑灰色---------------------------------*/
.g-bc,.def-h:hover
{background-color: #E0E0E0}
.g-fc{color: #E0E0E0}
.g-bdc,.but-cancel{border-color: #E0E0E0}

.g-bc1,.ngdialog.ngdialog-theme-default .ngdialog-content{background-color: #F3F3F3}
.g-fc1{color: #F3F3F3}
.g-bc1-tm{border-color:rgba(243,243,243,1.0);}
.g-bdc1{border-color: #F3F3F3}

.g-bc2{background-color: #7A7A7A}
.g-fc2{color: #7A7A7A}
.g-bdc2{border-color: #7A7A7A}

.g-bc3,button[disabled],button[disabled]:hover{background-color:#999999}
.g-fc3,.g-fc3-h,.ipt-txt>span{color: #999999}
