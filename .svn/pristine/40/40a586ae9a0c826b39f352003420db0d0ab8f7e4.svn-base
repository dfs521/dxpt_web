/*-----章节树---------------------------------*/
.tree>li p{height: 30px;line-height: 30px}
.tree>li p>span:first-child{font-weight: bold;}
.tree .tt-at>span:first-child{color: black}
.tree div{-webkit-transition:height .4s;transition:height .4s;overflow: hidden;height: 0;}
.tree div>p{padding-left: 2em}
.tree .active{border-style: solid;border-width: 1px;}

/*-----分页栏---------------------------------*/
.ng-pagination{position: relative;}
.ng-pagination>ul{margin-right: 200px;}
.ng-pagination>ul>li{padding: 6px 14px;margin-right:2px;display: inline-block;cursor: pointer;min-width: 37px;text-align: center;}
.ng-pagination>ul>li:hover,
.ng-pagination>ul>li.active{color: white;}
.ng-pagination>p{position: absolute;right: 0;top: 0;padding-right: 1em;
    height: 100%;line-height: 32px;width: 200px;text-align: right;font-size: 12px}
.ng-pagination input {
  margin-left: 2px;width: 30px;border-radius: 2px;border: 1px solid #ddd;
  vertical-align: 1px;text-align: center;padding: 6px 3px;}

/*-----表格---------------------------------*/
.m-tb{width:100%;border:1px solid #DDD;text-align:left;cursor: default;}
.m-tb>thead{color: gray}
.m-tb>thead th:first-child,.m-tb>tbody td:first-child{padding-left:1em;}
.m-tb>thead th:last-child,.m-tb>tbody td:last-child{text-align:right;padding-right:1em}
.m-tb thead th{text-align:left;padding-top:8px;padding-bottom:8px }
.m-tb tr{border-bottom:1px solid #DDD;}
.m-tb tbody td{padding-top:11px;padding-bottom:11px}
.m-tb tbody td:last-child{font-size: 16px}
.m-tb>tfoot td{} 

/*弹出层显示样式*/
.popopen,.popclose{-webkit-transition: opacity 0.5s, -webkit-transform 0.5s;
transition: opacity 0.5s, transform 0.5s, visibility 0.5s;}
.popopen{visibility: visible;opacity: 1;-webkit-transform: rotateX(0deg);transform: rotateX(0deg);}
.popclose{visibility: hidden;opacity: 0;-webkit-transform: rotateX(60deg);transform: rotateX(60deg);}

/*提示窗口*/
#msg{color:white;position:absolute;z-index:920;width:100%;color: white;top:10%;}
#msg>div{width: 470px;margin:0 auto;padding:5px 0}
#msg .m-mgs-head{height: 40px;}
#msg .m-mgs-head>p{float: left;}
#msg .m-mgs-head>p:first-child{font-size: 27px}
#msg .m-mgs-head>p:last-child{font-size: 18px;height: 100%;
    padding-left:.5em;line-height: 39px; }
#m-msg-cot{clear: both;font-size: 13px;padding: 5px 0 7px 0;line-height: 22px}
#msg .m-msg-foot{text-align: right;}            
#msg .m-msg-foot>button{width:100px;text-align:center;cursor:pointer;border:solid 2px white;background-color: transparent;padding: 1px 16px;height: auto;width: auto;}
#msg .m-msg-foot>button:hover{background-color: white;color: black;}

/*通知窗口*/
#notify{position:absolute;z-index: 918;width: 100%;height:100%;
    background-color: rgba(0,0,0,.1);top:0px;left: 0px; }
#notify>div{height: 80px;font-size: 20px;position: absolute;z-index: 919;right: 0;bottom: 0;line-height: 80px;
    -webkit-border-bottom-left-radius:40px ;-webkit-border-top-left-radius: 40px;border-bottom-left-radius:40px ;border-top-left-radius: 40px;text-align: center;
    transition: .5s width;-webkit-transition: .5s width;}
#notify .icon-check{font-size: 30px;color: green;}
.notify-show{display: block;}
.notify-close{display: none;}
.notify_w100{width: 100px;background-color: white;}
.notify_w200{width: 200px;color: white;background-color: green;}

/*图片查看窗口*/
#imgBox{position: fixed;height: 100%;width:100%;background-color: black;z-index: 917;top:0;left: 0;text-align: center}
#imgBox .menu{color: white;background-color:rgba(0,0,0,.6);position: absolute;top: 1em;right: 1em;}
#imgBox .menu>span{font-size: 20px;display: block;width: 50px;text-align: center;height: 50px;line-height: 50px}
#imgBox img{width:100%; height: 100%}
#imgBox .menu>span:hover{background-color: rgba(0,0,0,.4);}
.imgBox-open{display: block;}
.imgBox-close{display: none;}

/*视图切换动画*/
.slidedown,.slide,.slideup,.slideright
{position: absolute;top: 0;left: 0;width: 100%;height: 100%;}
.slide.ng-enter,.slide.ng-leave,
.slideright.ng-enter,.slideright.ng-leave,
.slidedown.ng-enter,.slidedown.ng-leave,
.slideup.ng-enter,.slideup.ng-leave,
.fade.ng-enter,.fade.ng-leave,
.pop.ng-enter,.pop.ng-leave,
.slide-pop.ng-enter,.slide-pop.ng-leave
{-webkit-transition: all .8s ease;transition: all .8s ease;}

.slideright.ng-enter {left: -100%;}
.slideright.ng-enter-active {left: 0;}
.slideright.ng-leave {left: 0;}
.slideright.ng-leave-active {left: 100%;}

.slide.ng-enter {left: 100%;}
.slide.ng-enter-active {left: 0;}
.slide.ng-leave {left: 0;}
.slide.ng-leave-active {left: -100%;}

.slidedown.ng-enter {top: 100%;}
.slidedown.ng-enter-active {top: 0;}
.slidedown.ng-leave {top: 0;}
.slidedown.ng-leave-active {top: 100%;} 

.slideup.ng-enter {top: 100%;}
.slideup.ng-enter-active {top: 0;}
.slideup.ng-leave {top: 0;}
.slideup.ng-leave-active {top: -100%;}

.fade {position: absolute;top: 0;left: 0;right: 0;height: 100%;opacity: 1;}
.fade.ng-enter {opacity: 0;}
.fade.ng-enter-active {opacity: 1;}
.fade.ng-leave {opacity: 1;}
.fade.ng-leave-active { opacity: 0;}

.pop {position: absolute;left: 0;right: 0;top: 0;bottom: 0;}
.pop.ng-enter {-webkit-transform: scale(0);transform: scale(0);}
.pop.ng-enter-active,
.pop.ng-leave,
.pop.ng-leave-active {-webkit-transform: scale(1);transform: scale(1);}
.pop.ng-leave-active {}

.slide-pop {left: 0;top: 0;width: 100%;height: 100%;}
.slide-pop.ng-enter {opacity: 0;-webkit-transform: scale(.5);transform: scale(.5);}
.slide-pop.ng-enter-active {opacity: 1;-webkit-transform: scale(1);transform: scale(1);}
.slide-pop.ng-leave {left: 0;opacity: .5;}
.slide-pop.ng-leave-active {left: -100%;opacity: 0;}
.reverse .page.slide-pop.ng-enter {opacity: .5;left: -100%;-webkit-transform: scale(1);transform: scale(1);}
.reverse .page.slide-pop.ng-enter-active {opacity: 1;left: 0;}
.reverse .page.slide-pop.ng-leave {opacity: .5;-webkit-transform: scale(1);transform: scale(1);left: 0;right: 0;}
.reverse .page.slide-pop.ng-leave-active {opacity: 0;-webkit-transform: scale(.5);transform: scale(.5);}

.rotate {-webkit-animation-duration: 1s;}
.ng-leave.rotate {-webkit-animation-name: rotate-leave;}
.ng-enter.rotate {-webkit-animation-name: rotate-enter;}
@-webkit-keyframes rotate-leave {
    0% {
        -webkit-transform-origin: center center;
        -webkit-transform: rotate(0);
        opacity: 1;
    }
    100% {
        -webkit-transform-origin: center center;
        -webkit-transform: rotate(-180deg);
        opacity: 0;
    }
}
@-webkit-keyframes rotate-enter {
    0% {
        -webkit-transform-origin: center center;
        -webkit-transform: rotate(-180deg);
        opacity: 0;
    }
    100% {
        -webkit-transform-origin: center center;
        -webkit-transform: rotate(-360deg);
        opacity: 1;
    }
}

